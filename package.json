{
  "name": "shades-of-purple-spotify-theme",
  "version": "0.1.1",
  "description": "Spotify colour scheme inspired by the Shades of Purple VSCode theme",
  "main": "user.css",
  "scripts": {
    "build": "parcel build src/_theme.less --out-dir . --out-file user.css --no-source-maps",
    "watch": "parcel watch src/_theme.less --out-dir . --out-file user.css --no-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theRealPadster/shades-of-purple-spotify-theme.git"
  },
  "keywords": [
    "spotify",
    "spotify-theme",
    "spicetify",
    "spicetify-cli"
  ],
  "bugs": {
    "url": "https://github.com/theRealPadster/shades-of-purple-spotify-theme/issues"
  },
  "homepage": "https://github.com/theRealPadster/shades-of-purple-spotify-theme#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.5"
  }
}
